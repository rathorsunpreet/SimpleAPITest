{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": [
      "airbnb-base",
      "plugin:mocha/recommended"
    ],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
      "prefer-arrow-callback": "off",
      "func-names": "off"
    },
    "plugins": [
      "mocha"
    ]
}
